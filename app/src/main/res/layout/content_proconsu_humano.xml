<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="proyecto.sugarapp.ProconsuHumano"
    tools:showIn="@layout/app_bar_proconsu_humano"
    android:id="@+id/contenedor">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/tipro"
        android:layout_alignParentTop="true"
        android:textAlignment="center"
        android:textSize="20sp"
        android:layout_margin="20dp"
        android:textStyle="bold"
        android:textColor="#000"/>


    <LinearLayout
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:orientation="vertical"
        android:id="@+id/conimg"
        android:layout_centerInParent="true">

        <ViewFlipper
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/contenimg">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/guaredondo"
                android:adjustViewBounds="true"
                android:scaleType="fitXY"/>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/mielredonda"
                android:adjustViewBounds="true"
                android:scaleType="fitXY"/>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/blanqueadoredondo"
                android:adjustViewBounds="true"
                android:scaleType="fitXY"/>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/carameloredondo"
                android:adjustViewBounds="true"
                android:scaleType="fitXY"/>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/iconpanela"
                android:adjustViewBounds="true"
                android:scaleType="fitXY"/>


        </ViewFlipper>




    </LinearLayout>

   <!-- <ImageView
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:src="@drawable/imgproducto"
        android:id="@+id/imgpro"
        android:layout_centerInParent="true"/>-->

</RelativeLayout>
