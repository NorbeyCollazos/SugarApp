<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context="proyecto.sugarapp.Inicio"    android:background="@drawable/fondobienvenida">    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="350dp"        android:orientation="vertical"        android:background="@drawable/imageninicio"        android:id="@+id/linearLayout">        <ImageView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:id="@+id/imageView"            android:src="@drawable/bienveni"            android:background="#712b2e2d"            android:layout_alignParentStart="true" />        <LinearLayout            android:orientation="horizontal"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="#9a7bb68e"            android:paddingTop="0dp"            android:paddingBottom="20dp"            android:id="@+id/linearLayout2"            android:layout_below="@+id/imageView"            android:layout_alignParentStart="true">            <Button                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="@string/culti"                android:id="@+id/btncultivo"                android:padding="10dp"                android:layout_weight="1"                android:textColor="#000"                android:textStyle="bold"                android:textAlignment="center"                android:background="@drawable/fondoboton2"                android:textAllCaps="false"                android:layout_marginRight="0dp"/>            <Button                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="@string/produc"                android:id="@+id/btnproductos"                android:layout_weight="1"                android:padding="10dp"                android:textColor="#000"                android:textStyle="bold"                android:textAlignment="center"                android:background="@drawable/fondoboton"                android:textAllCaps="false"                android:layout_marginRight="0dp"/>            <Button                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="@string/servicios"                android:id="@+id/btncontacto"                android:layout_weight="1"                android:padding="10dp"                android:textColor="#000"                android:textStyle="bold"                android:textAlignment="center"                android:background="@drawable/fondoboton"                android:textAllCaps="false"/>        </LinearLayout>    </RelativeLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="#712b2e2d"        android:id="@+id/franja"        android:layout_alignParentBottom="true"        android:layout_alignParentStart="true">        <ImageView            android:layout_width="100dp"            android:layout_height="100dp"            android:src="@drawable/logosugarapp2"            android:layout_alignParentBottom="true"            android:layout_alignParentStart="true"            android:layout_marginBottom="0dp" />        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="@string/tprin"            android:layout_gravity="center"            android:textColor="#fff"            android:textSize="16sp"            android:layout_marginLeft="10dp" />    </LinearLayout></RelativeLayout>